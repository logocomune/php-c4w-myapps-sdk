{
  "name": "logocomune/c4w-myapps-sdk",
  "type": "library",
  "description": "Unofficial My Apps SDK",
  "keywords": [
    "sdk",
    "cloud4wi"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Alessandro Lucaferro",
      "email": "alessandro.lucaferro@gmail.com",
      "homepage": "https://github.com/logocomune"
    }
  ],
  "require": {
    "php": ">=5.5",
    "guzzlehttp/guzzle": "~6.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.0",
    "squizlabs/php_codesniffer": "*"
  },
  "suggest": {
    "logocomune/debug": "Debug tool"
  },
  "autoload": {
    "psr-4": {
      "MyAppsSdk\\": "src/MyAppsSdk"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MyAppsSdk\\Tests\\": "tests/"
    }
  }
}
